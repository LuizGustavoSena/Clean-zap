{
    "manifest_version": 3,
    "name": "Clean zap",
    "version": "1.0.1",
    "description": "Cleaner your WhatsApp web page",
    "permissions": [
        "scripting",
        "activeTab",
        "tabs",
        "extension.js"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://web.whatsapp.com/"
            ],
            "js": [
                "script.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "CleanZap16.jpg",
        "32": "CleanZap32.jpg",
        "48": "CleanZap48.jpg",
        "128": "CleanZap128.jpg"
    }
}